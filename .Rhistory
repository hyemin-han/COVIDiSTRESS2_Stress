# function for factor score adjustment
aligned.factor.scores <- function(lambda,nu,y){
# calculate inverse matrix
lambda1 <- ginv((lambda))
# create matrix for nu
ns <- nrow(y)
nus <- matrix(nu,nrow=ns,ncol=length(nu),byrow=T)
# y - nu
y_nu <- y - nus
# f = inv(lambda)*(y-nu)
F <- lambda1 %*% t(as.matrix(y_nu))
}
